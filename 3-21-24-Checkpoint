    "columns_to_use = ['Season', 'FG', 'FGA', 'FG%', '3P', '3PA', '3P%', '2P', '2PA', '2P%', 'FT', 'FTA', 'FT%', 'ORB', 'DRB', 'TRB', 'AST', 'STL', 'BLK', 'TOV', 'PF', 'PTS']\n",
    "\n",
    "# Load the CSV file with only the selected columns\n",
    "Opponents_df = pd.read_csv('Knicks_Opps_Per_Game_data.txt', usecols=columns_to_use)\n",
    "\n",
    "# Extract the starting year of each season\n",
    "Opponents_df['Starting_Year'] = Opponents_df['Season'].str.split('-').str[0]\n",
    "\n",
    "for col in columns_to_use[1:]:\n",
    "    Opponents_df[col] = Opponents_df[col].astype(float)\n",
    "\n",
    "# Filter the DataFrame to exclude rows before the 1979 season\n",
    "Opponents_df= Opponents_df[Opponents_df['Starting_Year'].astype(int) >= 2000]\n",
    "\n",
    "Opponents_df['FG%'] = All_Seasons_df['FG%']\n",
    "Opponents_df['3P%'] = All_Seasons_df['3P%']\n",
    "Opponents_df['2P%'] = All_Seasons_df['2P%']\n",
    "Opponents_df['FT%'] = All_Seasons_df['FT%']\n",
    "\n",
    "\n",
    "# Drop the \"Starting_Year\" column\n",
    "Opponents_df.drop(columns=['Starting_Year'], inplace=True)\n",
    "\n",
    "pd.set_option('display.max_columns', None)\n",
    "\n",
    "# Print the entire DataFrame\n",
    "print(Opponents_df)"
   ]
